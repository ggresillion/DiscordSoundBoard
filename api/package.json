{
  "name": "discord-sound-board",
  "version": "0.0.0",
  "description": "A web client discord sound board for Discord !",
  "author": "Guillaume Gr√©sillion",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.3",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.5.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/platform-socket.io": "^7.5.1",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/typeorm": "^7.1.4",
    "@nestjs/websockets": "^7.5.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "discord.js": "^12.4.1",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^4.2.7",
    "fluent-ffmpeg": "^2.1.2",
    "get-audio-duration": "^2.0.3",
    "install": "^0.13.0",
    "node-fetch": "^2.6.1",
    "node-opus": "^0.3.1",
    "npm": "^6.14.8",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "scrape-youtube": "^2.0.1",
    "typeorm": "^0.2.29",
    "typescript": "^4.0.5",
    "uuid": "^8.3.1",
    "ytdl-core": "^4.0.3",
    "ytsr": "^1.0.4",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.5.1",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.8",
    "@types/fluent-ffmpeg": "^2.1.16",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@types/node-fetch": "^2.5.7",
    "@types/socket.io": "^2.1.11",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/youtube-dl": "^3.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.20.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
